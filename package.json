{
  "name": "react-native-eva-icons",
  "version": "1.0.0",
  "description": "Eva Icons for React Native",
  "scripts": {
    "start": "npm run gen:prepare && npm run gen:all && npm run lint:all",
    "build": "tsc",
    "clean": "rimraf ./src",
    "lint:tslint": "tslint -c ./tslint.json ./src/**/*.{ts,tsx} --fix",
    "lint:prettier": "prettier ./src/**/*.{ts,tsx} --write",
    "lint:all": "npm run lint:prettier && npm run lint:tslint",
    "gen:prepare": "mkdir src",
    "gen:fill": "ts-node ./scripts/generate.ts ./node_modules/eva-icons/fill/svg ./src",
    "gen:outline": "ts-node ./scripts/generate.ts ./node_modules/eva-icons/outline/svg ./src",
    "gen:index": "ts-node ./scripts/generateIndex.ts ./src",
    "gen:all": "npm run gen:fill && npm run gen:outline && npm run gen:index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artyorsh/react-native-eva-icons.git"
  },
  "keywords": [
    "react-native",
    "eva-icons",
    "svg"
  ],
  "author": "artyorsh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artyorsh/react-native-eva-icons/issues"
  },
  "homepage": "https://github.com/artyorsh/react-native-eva-icons#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.0.2",
    "@types/stringify-object": "^3.2.0",
    "eva-icons": "^1.1.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "stringify-object": "^3.3.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "react": "16.5.0",
    "react-native-svg": "^9.4.0"
  }
}
